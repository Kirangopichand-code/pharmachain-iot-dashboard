        "format": "<script>\n(function(scope) {\n    scope.$watch('msg.payload', function(payload) {\n        try {\n            var mapDiv = document.getElementById(\"mapDiv\");\n            if (mapDiv && typeof payload === \"string\") {\n                mapDiv.innerHTML = payload;\n            }\n        } catch(e) {\n            console.error(e);\n        }\n    });\n})(scope);\n</script>\n\n<div id=\"mapDiv\" style=\"text-align:center;\"></div>\n",
