    {
        "id": "9e9ef26eb4a60a4c",
        "type": "function",
        "z": "25ee7ada6aa83faf",
        "name": "function 1",
        "func": "if (msg.payload.latitude && msg.payload.longitude) {\n    const lat = msg.payload.latitude;\n    const lon = msg.payload.longitude;\n    const batch = msg.payload.batch_id || \"Unknown Batch\";\n    const stage = msg.payload.stage || \"Unknown Stage\";\n\n    msg.payload = `\n        <div style=\"text-align:center;\">\n            <iframe\n                width=\"100%\"\n                height=\"300\"\n                style=\"border:0; border-radius:12px;\"\n                loading=\"lazy\"\n                allowfullscreen\n                referrerpolicy=\"no-referrer-when-downgrade\"\n                src=\"https://maps.google.com/maps?q=${lat},${lon}&z=14&output=embed\">\n            </iframe>\n            <p style=\"margin-top:8px;font-weight:bold;font-size:14px;\">\n                üßæ ${batch} ‚Äî üöö ${stage}<br>\n                üìç Lat: ${lat.toFixed(4)}, Lon: ${lon.toFixed(4)}\n            </p>\n        </div>`;\n}\nreturn msg;\n",
